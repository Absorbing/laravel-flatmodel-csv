{
    "name": "flatmodel/laravel-csv-flatmodel",
    "description": "Eloquent-inspired data modeling system for Laravel that works with CSV files",
    "keywords": ["laravel", "csv", "flatfile", "eloquent", "model", "data"],
    "type": "library",
    "license": "MIT",
    "require": {
        "illuminate/collections": "^12.14",
        "illuminate/support": "^12.14"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "orchestra/testbench": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "FlatModel\\CsvModel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FlatModel\\CsvModel\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Absorbing"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "FlatModel\\CsvModel\\CsvModelServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    }
}
